{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "prestart": "powershell -ExecutionPolicy Bypass -File ./scripts/kill-port.ps1 -Port 3000",
    "start": "node dist/server.js",
    "kill-port": "powershell -ExecutionPolicy Bypass -File ./scripts/kill-port.ps1 -Port 3000",
    "start:clean": "npm run kill-port && npm start",
    "import:tokyo": "ts-node src/scripts/importTokyoShops.ts",
    "import:japan": "ts-node src/scripts/importJapanShopsOSM.ts",
    "analyze:locations": "ts-node src/scripts/analyzeShopsByLocation.ts"
  },
  "dependencies": {
    "@line/bot-sdk": "^10.5.0",
    "@supabase/supabase-js": "^2.81.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^144.0.0",
    "multer": "^1.4.5-lts.1",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.13",
    "@types/node": "^20",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vercel": "^28.18.5"
  }
